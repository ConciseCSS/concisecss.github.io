@import 'settings.ccss';
@import '../../node_modules/concise.css/concise';

h1 {
  margin-top: 3lh;
}

h2 {
  margin-top: 2lh;
}

h3 {
  text-transform: uppercase;
  font-size: typeScale(2);
  margin-bottom: 0.1lh;
}

a {
  font-feature-settings: "smcp" 1, "c2sc" 1;
  color: getColor(text, primary);
  letter-spacing: 0.05em;
}
